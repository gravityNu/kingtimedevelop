<Window x:Class="kingofTime.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kingofTime"
        mc:Ignorable="d"
        Background="Black"
        Title="kingofTime" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Border BorderThickness="1" BorderBrush="White" Grid.Row="0">
            <TextBlock Text=" king of Time Automation" Foreground="White" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        </Border>
        <Border BorderThickness="1" BorderBrush="White" Grid.Row="1"/>
        <Border BorderThickness="1" BorderBrush="White" Grid.Row="2"/>
        <Border BorderThickness="1" BorderBrush="White" Grid.Row="3"/>
        <Border BorderThickness="1" BorderBrush="White" Grid.Row="4"/>
        <Border BorderThickness="1" BorderBrush="White" Grid.Row="5"/>
    </Grid>
</Window>
